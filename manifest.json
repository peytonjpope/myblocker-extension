{
  "name": "MyBlocker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
  ],
  "permissions": [
      "storage",
      "activeTab"
  ],
  "web_accessible_resources": [
      {
        "resources": ["blocked.html", "index.html"],
        "matches": ["<all_urls>"]
      }
  ]
}