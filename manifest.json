{
  "name": "MyBlocker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup-page/popup.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
  ],
  "permissions": [
      "storage",
      "activeTab"
  ],
  "web_accessible_resources": [
      {
        "resources": ["blocked-page/blocked.html", "home-page/index.html"],
        "matches": ["<all_urls>"]
      }
  ]
}